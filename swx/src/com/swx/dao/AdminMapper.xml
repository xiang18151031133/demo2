<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.swx.dao.AdminMapper">

    <select id="getAdmin" resultType="com.swx.model.Admin">
        select *
        from T_ADMIN
        where a_name=#{a_name} and a_pass=#{a_pass}
    </select>
</mapper>